    "ts-patch": "^3.2.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3",
    "typescript-transform-paths": "^3.4.7"
  },
  "dependencies": {
    "@jest/globals": "^29.7.0",
    "@octokit/types": "^13.5.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "figlet": "^1.7.0",
    "octokit": "^4.0.2",
    "pg-promise": "^11.9.1",
    "terminal-image": "^2.0.0",
    "tiny-invariant": "^1.3.3"
